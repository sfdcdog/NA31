    <validationRules>
        <fullName>Core_IPS_Prevent_Edit_Until_Complete</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(Core_Upload_Status__c, &apos;Complete&apos;)),
($Profile.Name &lt;&gt; $Label.Core_System_Admin_Profile_Name),
($Profile.Name &lt;&gt; $Label.Core_IT_Admin_Profile_Name),
($Profile.Name &lt;&gt; $Label.Core_Integration_Profile_Name),
($Profile.Name &lt;&gt; $Label.Core_IPS_DataMigration_ProfileName)
)</errorConditionFormula>
        <errorMessage>No Edits allowed until File Upload Status is set to &apos;Complete&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Core_IPS_Prevent_StatusChng_FromComplete</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(PRIORVALUE( Core_Upload_Status__c ),&quot;Complete&quot;), NOT(ISPICKVAL( Core_Upload_Status__c ,&quot;Complete&quot;))
)</errorConditionFormula>
        <errorMessage>Status Change not allowed once set to Complete</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Core_Assign_SP_Account</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign SP/Account</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var ptb = new sforce.SObject(&quot;Core_IPS_File_Upload_Master__c&quot;);
ptb.Id = &apos;{!Core_IPS_File_Upload_Master__c.Id}&apos;; 
var IPSFileUploadID = &apos;{!Core_IPS_File_Upload_Master__c.Id}&apos;; 
var IPSFileUploadStatus = &apos;{!Core_IPS_File_Upload_Master__c.Core_Upload_Status__c}&apos;;

if(IPSFileUploadStatus != &apos;Complete&apos;){
alert(&apos;No Supported Products or Accounts can be assigned to this IPS Flie Upload since the status is not Complete&apos;); 
}
else{
window.open(&quot;/apex/Core_IPS_File_AssignSPAccount&quot;+&quot;?id=&quot;+IPSFileUploadID, &apos;_self&apos;);
}</url>
    </webLinks>
