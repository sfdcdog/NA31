    <validationRules>
        <fullName>Core_IPS_Env_Dtls_Master_Ans_No_Blanks</fullName>
        <active>true</active>
        <description>Ensures that when a datatype of Picklist or Multi Select is selected, there must be at least one row of text in the Answer text box.</description>
        <errorConditionFormula>AND
(OR(
ISPICKVAL(Core_Datatype__c, &apos;picklist&apos;), ISPICKVAL(Core_Datatype__c, &apos;multi select&apos;)
),
 ISBLANK(Core_Answers__c) 
)</errorConditionFormula>
        <errorDisplayField>Core_Answers__c</errorDisplayField>
        <errorMessage>Enter at least 1 row of text when Datatype is &apos;Picklist&apos; or &apos;Multi Select&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Core_IPS_Env_Dtls_Master_Choose_DataType</fullName>
        <active>false</active>
        <description>Ensure the DataType field cannot be blank. Input is mandatory.</description>
        <errorConditionFormula>ISBLANK (TEXT(Core_Datatype__c))</errorConditionFormula>
        <errorDisplayField>Core_Datatype__c</errorDisplayField>
        <errorMessage>Datatype must be specified.</errorMessage>
    </validationRules>
