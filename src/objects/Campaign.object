    <validationRules>
        <fullName>Core_Campaign_Only_Approved_In_Progress</fullName>
        <active>true</active>
        <description>Only allow Sales Campaigns that are approved to be set to status of In Progress</description>
        <errorConditionFormula>(RecordType.DeveloperName = &apos;Core_Sales_Campaign&apos;) &amp;&amp;
(ISCHANGED(Status) || ISNEW()) &amp;&amp;
(ISPICKVAL(Status, &apos;In Progress&apos;)) &amp;&amp;
(NOT(ISPICKVAL(Core_Approval_Status__c, &apos;Approved&apos;)))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Sales Campaigns must be approved before being set to &quot;In Progress&quot;</errorMessage>
    </validationRules>
