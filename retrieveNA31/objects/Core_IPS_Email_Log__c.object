        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Core_IPS_Email_Log__c)};
if (records.length == 0) 
{alert(&quot;Please select at least one Record&quot;)} 
else if (records.length &gt; 50) {alert(&quot;Please select no more than 50 records&quot;)}
else {
resstring = sforce.apex.execute(&quot;Core_IPS_Email_Reprocess&quot;, &quot;ReprocessExtDownloadNotifEmail&quot;,{RecordIds:records});
alert(resstring);
window.location.reload();
}</url>
